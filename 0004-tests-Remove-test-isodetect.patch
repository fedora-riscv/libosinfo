-static void free_iso(struct ISOInfo *info)
-{
-    if (!info)
-        return;
-
-    g_free(info->filename);
-    g_free(info->shortid);
-    if (info->media)
-        g_object_unref(info->media);
-    if (info->langs)
-        g_hash_table_unref(info->langs);
-    g_free(info);
-}
